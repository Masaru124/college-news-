{
  "builds": [{
      "src": "manage.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb", 
        "runtime": "python3.9", 
        "includeFiles": [".env"] 
      }
  }],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "manage.py"
      }
  ]
}
